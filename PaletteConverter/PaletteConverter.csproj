<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <AllowUnsafeBlocks>False</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="HtmlAgilityPack" Version="1.12.1" />
    <PackageReference Include="Selenium.WebDriver" Version="4.32.0" />
    <PackageReference Include="WebDriverManager" Version="2.17.5" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="FormPluginsContracts">
      <HintPath>..\..\FormPluginsContracts\FormPluginsContracts\bin\Debug\net8.0-windows\FormPluginsContracts.dll</HintPath>
    </Reference>
    <Reference Include="PalettePluginContracts">
      <HintPath>..\..\PalettePluginContracts\PalettePluginContracts\bin\Debug\net8.0\PalettePluginContracts.dll</HintPath>
    </Reference>
    <Reference Include="ParserContracts">
      <HintPath>..\..\ParserContracts\ParserContracts\bin\Debug\net8.0\ParserContracts.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>